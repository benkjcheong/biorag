{
  "publication": {
    "pmc_id": "PMC3630201",
    "title": "Microgravity Induces Pelvic Bone Loss through Osteoclastic Activity, Osteocytic Osteolysis, and Osteoblastic Cell Cycle Inhibition by CDKN1a/p21",
    "year": "2013",
    "journal": "PLOS One"
  },
  "authors": [
    "Blaber, Elizabeth A.",
    "Dvorochkin, Natalya",
    "Lee, Chialing",
    "Alwood, Joshua S.",
    "Yousuf, Rukhsana",
    "Pianetta, Piero",
    "Globus, Ruth K.",
    "Burns, Brendan P."
  ],
  "subjects": {
    "species": [
      "C57BL/6J mouse"
    ],
    "tissues": [
      "bone",
      "pelvis",
      "femur"
    ]
  },
  "methods": {
    "platforms": [
      "STS-131 space shuttle mission"
    ],
    "assays": [
      "\u00b5CT",
      "high-resolution X-ray nanoCT studies",
      "RT-qPCR",
      "immunohistochemistry"
    ]
  },
  "treatments": [
    {
      "agent": "microgravity",
      "dose": null,
      "duration": "15 days"
    }
  ],
  "results": [
    {
      "target": "bone volume fraction (BV/TV)",
      "effect": "decreased",
      "magnitude": "6.29%"
    },
    {
      "target": "bone thickness",
      "effect": "decreased",
      "magnitude": "11.91%"
    },
    {
      "target": "TRAP-positive osteoclast-covered trabecular bone surfaces",
      "effect": "increased",
      "magnitude": "170%"
    },
    {
      "target": "lacunar osteolysis (cross-sectional area, perimeter, canalicular diameter)",
      "effect": "increased",
      "magnitude": "+17%, +14%, +6% respectively"
    },
    {
      "target": "matrix metalloproteinases (MMP) 1, 3, and 10 expression",
      "effect": "up-regulated",
      "magnitude": "+12.94, +2.98, +16.85 fold respectively"
    },
    {
      "target": "CDKN1a/p21 expression",
      "effect": "increased",
      "magnitude": "3.31 fold"
    },
    {
      "target": "Trp53 expression",
      "effect": "down-regulated",
      "magnitude": "-1.54 fold"
    }
  ]
}