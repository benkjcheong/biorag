    {
      "target": "hip cartilage volume and thickness",
      "effect": "decreased",
      "magnitude": "significant decrease compared to ground-sham control"
    },
    {
      "target": "collagen levels",
      "effect": "lowered",
      "magnitude": "all groups showed lower collagen compared to ground-sham"
    },
    {
